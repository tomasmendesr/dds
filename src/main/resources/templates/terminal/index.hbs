{{#partial "contenido"}}
<h1>Busqueda</h1>

<table class="table table-stripped">
	<tr>
		<th>Nombre</th>
		<th>Direccion</th>
		<th>Comuna</th>
		<th></th>
	</tr>
	
	{{#each pois}}
		<tr>
			<td>{{nombre}}</td>
			<td>{{direccion}}</td>
			<td>{{comuna}}</td>
			
		</tr>
	{{/each}}
</table>
{{/partial}}
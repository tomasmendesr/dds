{{#partial "contenido"}}
<h1>Busqueda</h1>

<table class="table table-stripped">
	<tr>
		<th>Nombre</th>
		<th>Direccion</th>
		<th>Comuna</th>
		<th></th>
	</tr>
	
	{{#each terminal}}
		<tr>
			<td>{{Nombre}}</td>
			<td>{{Direccion}}</td>
			<td>{{Comuna}}</td>
			<td><a href="{{url}}">Ver detalles</a><td>
		</tr>
	{{/each}}
</table>
{{/partial}}
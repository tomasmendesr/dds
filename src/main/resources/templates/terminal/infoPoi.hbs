{{#partial "contenido"}}
{{#with poi}}
	
<h4>{{nombre}}</h4>
	<p><label>Direccion: </label>{{direccion}}</p>
	<p><label>Tipo: </label>{{tipoDePOI}}</p>
	<p><label>Coordenadas: X: </label>{{coordenadaX}}<label>- Y: </label>{{coordenadaY}} </p>
	<p><label>Comuna: </label>{{numeroComuna}}</p>
	<p><label> Servicios </label></p>
	<ul>
		{{#each servicios}}
  			<li>{{nombre}}</li>
  		{{/each}}
	</ul>
{{/with}}
<FORM><INPUT Type="button" VALUE="Volver" onClick="history.go(-1);return true;"></FORM>
{{/partial}}
{{>layout.hbs}}
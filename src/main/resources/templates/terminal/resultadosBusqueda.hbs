{{#partial "contenido"}}
{{#with terminal}}
<h3>POIs encontrados</h3>

<table class="table table-stripped">
	<tr>
		<th>Nombre</th>
		<th>Tipo</th>
		<th>Direccion</th>
		<th>Coordenadas</th>
		<th></th>
	</tr>
	
	{{#each pois}}
		<tr>
			<td>{{nombre}}</td>
			<td>{{tipoDePOI}}</td>
			<td>{{direccion}}</td>
			<td>X: {{coordenadaX}} - Y: {{coordenadaY}}</td>
			<td><a href="{{url}}"">Ver Mas</a><td>
		</tr>
	{{/each}}
</table>
<label><a href="{{url}}">Volver</a></label>
{{/with}}
{{/partial}}
{{>layout.hbs}}